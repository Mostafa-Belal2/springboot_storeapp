<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.storeapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".User.UpdateUser"/>
        <activity android:name=".User.ListUser"/>
        <activity android:name=".User.AddUser"/>
        <activity android:name=".User.Login"/>
        <activity android:name=".Customer.ListCusotmer"/>
        <activity android:name=".Customer.UpdateCustomer"/>
        <activity android:name=".Category.ListCategory"/>
        <activity android:name=".Category.UpdateCategory"/>
        <activity android:name=".Size.ListSize"/>
        <activity android:name=".Size.UpdateSize"/>
        <activity android:name=".Supplier.ListSupplier"/>
        <activity android:name=".Supplier.UpdateSupplier"/>
        <activity android:name=".SubCategory.ListSubCategory"/>
        <activity android:name=".SubCategory.UpdateSubCategory"/>


    </application>

</manifest>